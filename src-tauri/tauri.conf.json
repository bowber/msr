{
  "$schema": "https://schema.tauri.app/config/2",
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "MSR",
  "mainBinaryName": "MSR",
  "version": "0.1.1",
  "identifier": "com.msr.app",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "MSR",
        "width": 800,
        "height": 600,
        "decorations": false,
        "transparent": true,
        "useHttpsScheme": true
      }
    ]
  }
}
